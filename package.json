{
  "name": "cypress-system",
  "version": "1.0.0",
  "description": "Cypress 10 env var example",
  "main": "index.js",
  "scripts": {
    "cypress": "./node_modules/.bin/cypress open",
    "e2e:dev-solution1": "cypress run --e2e --config-file cypress/configs/dev-cypress.config.ts --headed",
    "e2e:staging-solution1": "cypress run --e2e --config-file cypress/configs/stg-cypress.config.ts --headed",
    "e2e:prod-solution1": "cypress run --e2e --config-file cypress/configs/prod-cypress.config.ts --headed",
    "e2e:dev-solution2": "cypress run --e2e --env deployment-env=dev --headed",
    "e2e:staging-solution2": "cypress run --e2e --env deployment-env=staging --headed",
    "e2e:prod-solution2": "cypress run --e2e --env deployment-env=prod --headed"
  },
  "keywords": [
    "Cypress10",
    "example",
    "SystemTest"
  ],
  "author": "Lee Harrold",
  "license": "ISC",
  "dependencies": {
    "cypress": "^10.6.0",
    "typescript": "^4.7.4"
  }
}
